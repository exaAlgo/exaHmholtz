      common /nekmpi/ mid,mp,nekcomm,nekgroup,nekreal
      integer         mid,mp,nekcomm,nekgroup,nekreal

      integer lglo_num
      parameter(lglo_num=((lx1+2)**ldim)*lelt)
      common /c_is1/ glo_num(lglo_num)
      integer*8       glo_num,ngv

      COMMON /SCNRS/ SC_NRS(LX1*LY1*LZ1*LELT*7)
      real           SC_NRS

      common /INTNRS/ getu, getp, llelt
      integer         getu, getp, llelt
